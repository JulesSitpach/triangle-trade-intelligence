{
  "name": "triangle-intelligence-platform",
  "version": "1.0.0",
  "description": "Triangle Intelligence Platform - Tariff volatility tracking and trade optimization through USMCA triangle routing",
  "main": "index.js",
  "scripts": {
    "dev": "next dev -p 3000 -H 0.0.0.0",
    "build": "next build --no-lint",
    "build:fast": "SKIP_TYPE_CHECK=true next build --no-lint",
    "build-fast": "set NODE_ENV=development && next build",
    "start": "next start",
    "dev:force": "for /f \"tokens=5\" %a in ('netstat -aon ^| findstr :3000') do taskkill /F /PID %a 2>nul && timeout 2 && next dev -p 3000",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:playwright": "playwright test",
    "test:playwright:ui": "playwright test --ui",
    "test:visual": "playwright test --project=desktop-chrome",
    "test:mobile": "playwright test --project=mobile-iphone",
    "test:e2e": "playwright test tests/e2e",
    "test:e2e:ui": "playwright test tests/e2e --ui",
    "test:css-validation": "playwright test tests/e2e/ui-validation.spec.js",
    "test:puppeteer": "node tests/puppeteer/dashboard-ui-tests.js",
    "test:ui-full": "npm run test:puppeteer",
    "test:ui-headless": "HEADLESS=true npm run test:puppeteer",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "echo ‚ùå NEVER USE THIS! It kills Claude Code! Use start-clean.bat instead && exit 1",
    "clean:port": "for /f \"tokens=5\" %a in ('netstat -aon ^| findstr :3000') do taskkill /F /PID %a 2>nul || echo No process on port 3000",
    "clean:safe": "npm run clean:port",
    "dev:fresh": "npm run clean:safe && npm run dev",
    "dev:safe": "npm run clean:port && npm run dev",
    "dev:3001": "next dev -p 3001 -H 0.0.0.0",
    "build:force": "echo WARNING: build:force disabled to prevent .next deletion && npm run build",
    "dev:robust": "npm run build && npm run dev:safe",
    "restart": "npm run clean:safe && npm run dev",
    "ingest:hs-codes": "node scripts/ingest-wits-hs-codes.js",
    "ingest:us-tariffs": "node scripts/ingest-usitc-tariffs.js",
    "enhance:existing-codes": "node scripts/enhance-existing-hs-codes.js",
    "ingest:all": "npm run enhance:existing-codes",
    "check-hardcoding": "bash .githooks/pre-commit",
    "semgrep:scan": "docker run --rm -v .:/code ghcr.io/semgrep/mcp -c .semgrep-rules.yml /code",
    "semgrep:hardcoding": "docker run --rm -v .:/code ghcr.io/semgrep/mcp -c .semgrep-rules.yml --include=\"hardcoded-*\" /code",
    "setup-protection": "bash scripts/setup-git-hooks.sh",
    "validate-enterprise": "npm run check-hardcoding && npm run semgrep:hardcoding && echo '‚úÖ Project is enterprise-ready - no hardcoding detected'",
    "css:check": "node .css-protection.js check",
    "css:approve": "node .css-protection.js approve",
    "css:status": "node .css-protection.js status",
    "css:lock": "npm run css:approve && echo 'üîí CSS files are now locked and protected'",
    "protection:full": "npm run css:check && npm run check-hardcoding && echo 'üõ°Ô∏è Full protection check passed'",
    "validate:smart": "node smart-validation-framework.js",
    "validate:discover": "node auto-discovery-validator.js",
    "validate:all": "npm run validate:discover && npm run validate:smart"
  },
  "keywords": [
    "trade",
    "tariffs",
    "usmca",
    "supply-chain",
    "triangle-routing",
    "intelligence"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.59.0",
    "@sendgrid/mail": "^8.1.6",
    "@stripe/react-stripe-js": "^5.0.0",
    "@stripe/stripe-js": "^8.0.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.1.2",
    "chokidar": "^4.0.3",
    "cookie": "^1.0.2",
    "csv-parser": "^3.2.0",
    "express-rate-limit": "^8.1.0",
    "formidable": "^3.5.4",
    "google-auth-library": "^10.3.0",
    "googleapis": "^160.0.0",
    "i18next": "^25.3.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.539.0",
    "mdb-reader": "^3.1.0",
    "next": "^14.2.5",
    "nodemailer": "^7.0.6",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^3.2.0",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.6.1",
    "resend": "^6.1.2",
    "rss-parser": "^3.13.0",
    "sharp": "^0.34.3",
    "stripe": "^19.0.0",
    "tesseract.js": "^6.0.1",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@playwright/mcp": "^0.0.39",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.11",
    "critters": "^0.0.23",
    "dotenv": "^17.2.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.5",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "madge": "^8.0.0",
    "node-fetch": "^2.7.0",
    "node-mocks-http": "^1.17.2",
    "playwright": "^1.55.0",
    "prettier": "^3.6.2",
    "puppeteer": "^23.11.1",
    "typescript": "5.9.2"
  }
}
