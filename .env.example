# Triangle Intelligence Platform - Environment Variables Template
# Copy this file to .env.local and fill in your actual values
# NEVER commit .env.local to version control

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Stripe Configuration (Use test keys for development)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_webhook_secret_here

# Stripe Price IDs (Create these in Stripe Dashboard â†’ Products)
# SMB-Focused Pricing: Starter ($99/$950), Professional ($299/$2850), Premium ($599/$5750)
STRIPE_PRICE_STARTER_MONTHLY=price_starter_monthly_your_price_id
STRIPE_PRICE_STARTER_ANNUAL=price_starter_annual_your_price_id
STRIPE_PRICE_PROFESSIONAL_MONTHLY=price_professional_monthly_your_price_id
STRIPE_PRICE_PROFESSIONAL_ANNUAL=price_professional_annual_your_price_id
STRIPE_PRICE_PREMIUM_MONTHLY=price_premium_monthly_your_price_id
STRIPE_PRICE_PREMIUM_ANNUAL=price_premium_annual_your_price_id

# Application URLs
NEXT_PUBLIC_APP_URL=http://localhost:3000

# AI and External APIs
ANTHROPIC_API_KEY=sk-ant-api03-your_anthropic_key
OPENROUTER_API_KEY=sk-or-v1-your_openrouter_key
COMTRADE_API_KEY=your_comtrade_api_key
SHIPPO_API_KEY=shippo_test_your_shippo_key
SAM_GOV_API_KEY=your_sam_gov_api_key

# Security Configuration
JWT_SECRET=your_secure_jwt_secret_minimum_32_characters
NEXTAUTH_SECRET=your_nextauth_secret_for_production
ENTERPRISE_MASTER_KEY=your_enterprise_master_key

# Development Configuration
NODE_ENV=development
USE_MOCK_APIS=false
AI_MODEL_PROFILE=PRODUCTION

# Security Notes:
# - Generate new JWT secrets using: openssl rand -base64 32
# - Use strong, unique passwords for all services
# - Enable 2FA on all external service accounts
# - Regularly rotate API keys (quarterly minimum)
# - Never share these credentials via email, chat, or screenshots